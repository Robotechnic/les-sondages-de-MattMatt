<!DOCTYPE html>
<html lang="fr">
<head>
	<meta charset="UTF-8">
	<title>Les sondages de MattMatt - Gestion</title>
	<%- include ("parts/include.ejs") %>
</head>
<body>
	<%- include ("parts/header.ejs") %>
	<h3>Mes sondages</h3>
	<section class="table-contener">
		<div>
			<table class="tableSondage">
				<thead>
					<tr>
						<td>Id</td>
						<td>Titre</td>
						<td>Réponses</td>
						<td>Actions</td>
					</tr>
				</thead>
				<tbody class="tableSondage__body">
					<% row.forEach((data) => {%>
						<tr>
							<td><%=data.id%></td>
							<td><%=data.name%></td>
							<td><%=data.responses%></td>
							<td>
								<button class="editButton">Editer</button>
								<button class="generateLinkButton">Créer un lien</button>
								<button class="deleteButton">Suprimmer</button>
							</td>
						</tr>
					<%})%>
				</tbody>
			</table>
		</div>
	</section>
	<%- include ("parts/footer.ejs") %>
</body>
</html>